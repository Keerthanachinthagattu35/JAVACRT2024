import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n,sum=0;
        n=s.nextInt();
       
    while(n>0)
    {
     int d=n%10;
        n=n/10;
        if(d%2!=0)
         sum=sum+d;
    }
    System.out.println(sum);
    }
}
